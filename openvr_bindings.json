            },
			{
               "inputs" : {
                  "position" : {
                     "output" : "/actions/demo/in/moving_xy"
                  }
               },
               "mode" : "trackpad",
               "path" : "/user/hand/right/input/trackpad"
            },
			{
               "inputs" : {
                  "position" : {
                     "output" : "/actions/demo/in/pressed_left_trackpad"
